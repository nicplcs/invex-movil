			android:icon="@drawable/logo"
			android:supportsRtl="true"
			android:theme="@style/Theme.AppInterface"
			android:usesCleartextTraffic="true"
			tools:targetApi="31">

		<activity
				android:name=".LoginActivity"
				android:exported="true">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>

		<activity
				android:name=".MainActivityy"
				android:exported="false">
		</activity>
		<!-- USUARIOSSS -->
		<activity
				android:name=".usuariosActivity"
				android:exported="false">
		</activity>

		<!-- PRODUCTOSSS -->
		<activity android:name=".ProductosActivity"
				  android:exported="true">
		</activity>

		<activity
				android:name=".Modulos_productosActivity"
				android:exported="false" />
		<activity
				android:name=".EntradaProductosActivity"
				android:exported="false" />
		<activity
				android:name=".SalidaProductosActivity"
				android:exported="false" />

		<!-- MOVIMIENTOSSSS -->
		<activity
				android:name=".MovimientosActivity"
				android:exported="false" />
		<activity
				android:name=".DevolucionesActivity"
				android:exported="false" />
		<activity
				android:name=".Modulos_movimientosActivity"
				android:exported="false" />

		<!-- PROVEEDORES -->
		<activity
				android:name=".ProveedoresActivity"
				android:exported="false" />

		<activity
				android:name=".Modulos_usuariosActivity"
				android:exported="false" />

		<!-- REGISTROOO -->

		<activity
				android:name=".RegistroActivity"
				android:exported="false" />

	</application>

</manifest>
